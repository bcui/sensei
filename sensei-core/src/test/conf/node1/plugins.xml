<?xml version="1.0" encoding="UTF-8"?>
<!-- sensei plugins -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="demo" class="com.sensei.search.nodes.impl.DemoZoieSystemFactory$DemoZoieFactoryFactory" />

  <bean id="beanRefFacets" class="java.lang.Integer">
    <constructor-arg ref="beanInFacets" />
  </bean>
  <bean id="beanInPlugins" class="java.lang.Integer">
    <constructor-arg value="1" />
  </bean>
</beans>
